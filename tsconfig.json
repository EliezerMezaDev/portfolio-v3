{
  "extends": "astro/tsconfigs/base",
  "include": ["**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/content/*": ["src/content/*"],
      "@/styles/*": ["src/styles/*"]
    }
  }
}
